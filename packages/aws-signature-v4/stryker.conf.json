{
	"packageManager": "pnpm",
	"mutate": [
		"./src/**/*.mts"
	],
	"ignorePatterns": [
		"**/node_modules/**",
		"**/.github/**",
		"**/.husky/**",
		"**/.stryker-tmp/**",
		"**/stats/**",
		"**/docs/**",
		"**/documentations/**",
		"**/reports/**",
		"**/coverage/**",
		"**/build/**",
		"**/dist/**",
		"**/lib/**"
	],
	"concurrency": 6,
	"reporters": [
		"html",
		"clear-text",
		"progress"
	],
	"htmlReporter": {
		"fileName": "./reports/stryker/mutation.html"
	},
	"jsonReporter": {
		"fileName": "./reports/stryker/mutation.json"
	},
	"commandRunner": {
		"command": "pnpm test:unit:stryker"
	},
	"coverageAnalysis": "perTest",
	"ignoreStatic": true,
	"tsconfigFile": "tsconfig.stryker.json"
}
