{
	"extends": [
		"../../.eslintrc.json"
	],
	"settings": {
		"import/resolver": {
			"typescript": {
				"alwaysTryTypes": true,
				"project": "./tsconfig.eslint.json"
			}
		}
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"parser": "@typescript-eslint/parser",
		"project": "./tsconfig.eslint.json",
		"projectFolderIgnoreList": [
			"./node_modules",
			"./.stryker-tmp",
			"./build",
			"./reports",
			"*.js"
		]
	}
}
